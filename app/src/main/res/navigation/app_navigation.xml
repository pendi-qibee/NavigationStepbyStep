<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/app_navigation"
            app:startDestination="@id/homeFragment">

    <fragment android:id="@+id/homeFragment" android:name="id.qibee.navigationstepbystep.HomeFragment"
              android:label="fragment_home" tools:layout="@layout/fragment_home">
        <action android:id="@+id/action_homeFragment_to_navigation_Dependency_Fragment"
                app:destination="@id/navigation_Dependency_Fragment"/>
    </fragment>
    <fragment android:id="@+id/navigation_Dependency_Fragment"
              android:name="id.qibee.navigationstepbystep.Navigation_Dependency_Fragment"
              android:label="fragment_navigation__dependency" tools:layout="@layout/fragment_home"/>
    <fragment android:id="@+id/actionFragment" android:name="id.qibee.navigationstepbystep.ActionFragment"
              android:label="fragment_action" tools:layout="@layout/fragment_home"/>
    <fragment android:id="@+id/argReceiverFragment" android:name="id.qibee.navigationstepbystep.ArgReceiverFragment"
              android:label="fragment_arg_receiver" tools:layout="@layout/fragment_home"/>
</navigation>